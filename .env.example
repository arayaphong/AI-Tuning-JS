# Google Cloud Configuration for Vertex AI
GOOGLE_CLOUD_PROJECT=your-google-cloud-project-id
GOOGLE_CLOUD_LOCATION=your-google-cloud-location
GOOGLE_CLOUD_VERTEX_MODEL=your-vertex-model

# Google Authentication
GOOGLE_AUTH_SCOPES=https://www.googleapis.com/auth/cloud-platform

# Google AI Configuration
GOOGLE_API_KEY=your-google-api-key
GOOGLE_AI_EMBEDDING_MODEL=embedding-001

# Vertex AI Embedding Configuration
VERTEX_AI_MODEL=text-embedding-004
VERTEX_AI_TASK_TYPE=RETRIEVAL_DOCUMENT

# Optional Vertex AI Embedding Parameters
VERTEX_AI_AUTO_TRUNCATE=true
VERTEX_AI_OUTPUT_DIMENSIONALITY=768

# Model Generation Configuration
MAX_OUTPUT_TOKENS=65535
TEMPERATURE=1
TOP_P=1
SEED=0

# Safety Settings
SAFETY_THRESHOLD=OFF

# Simple Embedding Configuration
SIMPLE_EMBEDDING_DIMENSIONS=768
SIMPLE_EMBEDDING_MAX_HASH_VALUE=65535.0

# Safety Settings
SAFETY_THRESHOLD=OFF
HARM_CATEGORY_HATE_SPEECH=HARM_CATEGORY_HATE_SPEECH
HARM_CATEGORY_DANGEROUS_CONTENT=HARM_CATEGORY_DANGEROUS_CONTENT
HARM_CATEGORY_SEXUALLY_EXPLICIT=HARM_CATEGORY_SEXUALLY_EXPLICIT
HARM_CATEGORY_HARASSMENT=HARM_CATEGORY_HARASSMENT

# Vertex AI Configuration
VERTEX_AI_INDEX_ID=your-project/global/indexes/your-index
VERTEX_AI_INDEX_ENDPOINT_ID=your-project/global/indexEndpoints/your-endpoint
VERTEX_AI_DEPLOYED_INDEX_ID=your-project/global/index

# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net

# Postgres Configuration
POSTGRES_URI=postgresql://username:password@host:port/database

# Development settings
NODE_ENV=development

# Note: You may also need to set GOOGLE_APPLICATION_CREDENTIALS
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json
